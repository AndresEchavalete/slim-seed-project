{
  "version": "1",
  "name": "Slim Seed Project API",
  "type": "collection",
  "ignore": [
    "node_modules",
    ".git"
  ],
  "folders": [
    {
      "id": "folder-1",
      "name": "üè† P√∫blicos",
      "items": [
        {
          "id": "req-1",
          "name": "Bienvenida",
          "type": "http",
          "method": "GET",
          "url": "{{baseUrl}}/",
          "headers": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        },
        {
          "id": "req-2",
          "name": "Health Check",
          "type": "http",
          "method": "GET",
          "url": "{{baseUrl}}/health",
          "headers": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        },
        {
          "id": "req-3",
          "name": "Status API",
          "type": "http",
          "method": "GET",
          "url": "{{baseUrl}}/api/status",
          "headers": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        }
      ]
    },
    {
      "id": "folder-2",
      "name": "üë• Usuarios",
      "items": [
        {
          "id": "req-4",
          "name": "Crear Usuario",
          "type": "http",
          "method": "POST",
          "url": "{{baseUrl}}/api/users",
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "json",
            "json": {
              "email": "{{testEmail}}",
              "name": "{{testName}}",
              "password": "{{testPassword}}"
            }
          }
        },
        {
          "id": "req-5",
          "name": "Obtener Usuarios Activos",
          "type": "http",
          "method": "GET",
          "url": "{{baseUrl}}/api/users/active",
          "headers": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        },
        {
          "id": "req-6",
          "name": "Obtener Usuario por ID",
          "type": "http",
          "method": "GET",
          "url": "{{baseUrl}}/api/users/{{userId}}",
          "headers": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        },
        {
          "id": "req-7",
          "name": "Actualizar Nombre de Usuario",
          "type": "http",
          "method": "PUT",
          "url": "{{baseUrl}}/api/users/{{userId}}/name",
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "json",
            "json": {
              "name": "Usuario Actualizado"
            }
          }
        },
        {
          "id": "req-8",
          "name": "Autenticar Usuario",
          "type": "http",
          "method": "POST",
          "url": "{{baseUrl}}/api/users/authenticate",
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "json",
            "json": {
              "email": "{{testEmail}}",
              "password": "{{testPassword}}"
            }
          }
        }
      ]
    },
    {
      "id": "folder-3",
      "name": "üìä Salud",
      "items": [
        {
          "id": "req-9",
          "name": "√öltimo Estado de Salud",
          "type": "http",
          "method": "GET",
          "url": "{{baseUrl}}/api/health/latest",
          "headers": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        },
        {
          "id": "req-10",
          "name": "Historial de Salud (√öltima Semana)",
          "type": "http",
          "method": "GET",
          "url": "{{baseUrl}}/api/health/history?from=2025-09-19&to=2025-09-26",
          "headers": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        },
        {
          "id": "req-11",
          "name": "Historial de Salud (√öltimo Mes)",
          "type": "http",
          "method": "GET",
          "url": "{{baseUrl}}/api/health/history?from=2025-08-26&to=2025-09-26",
          "headers": [
            {
              "name": "Accept",
              "value": "application/json"
            }
          ]
        }
      ]
    },
    {
      "id": "folder-4",
      "name": "üîî Notificaciones",
      "items": [
        {
          "id": "req-12",
          "name": "Enviar Alerta Simple",
          "type": "http",
          "method": "POST",
          "url": "{{baseUrl}}/api/notifications/alert",
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "json",
            "json": {
              "message": "Sistema en mantenimiento programado"
            }
          }
        },
        {
          "id": "req-13",
          "name": "Enviar Alerta con Contexto",
          "type": "http",
          "method": "POST",
          "url": "{{baseUrl}}/api/notifications/alert",
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ],
          "body": {
            "mode": "json",
            "json": {
              "message": "Mantenimiento programado del servidor",
              "context": {
                "duration": "2 hours",
                "affected_services": ["API", "Database", "Redis"],
                "severity": "high",
                "scheduled_time": "2025-09-26 15:00:00"
              }
            }
          }
        },
        {
          "id": "req-14",
          "name": "Notificaci√≥n de Prueba",
          "type": "http",
          "method": "POST",
          "url": "{{baseUrl}}/api/notifications/test",
          "headers": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        }
      ]
    },
